<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="empresa_contratadora_tree">
      <field name="name">proyecto.empresa_contratadora.tree</field>
      <field name="model">proyecto.empresa_contratadora</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name" />
          <field name="empleados" />
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="empresa_contratadora_form">
      <field name="name">proyecto.empresa_contratadora.form</field>
      <field name="model">proyecto.empresa_contratadora</field>
      <field name="arch" type="xml">
        <form>
          <group colspan="2" col="2">
            <field name="name" />
            <field name="empleados" />
          </group>
        </form>
      </field>
    </record>

    <!-- actions opening views on models -->
    
    <record model="ir.actions.act_window" id="empresa_contratadora.action_window">
      <field name="name">empresa_contratadora</field>
      <field name="res_model">proyecto.empresa_contratadora</field>
      <field name="view_mode">tree,form</field>
    </record>


